{
    "name": "silverstripe/recipe-serverless",
    "type": "silverstripe-recipe",
    "description": "SilverStripe framework-only core recipe",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/assets": "~2.0.0@stable",
        "silverstripe/config": "~2.0.0@stable",
        "silverstripe/framework": "~5.0.0@stable",
        "silverstripe/mimevalidator": "~3.0.0@stable",
        "bref/bref": "^2.0",
        "composer-plugin-api": "^2",
        "silverstripe/sqlite3": "^3.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "silverstripe/versioned": "^2",
        "mikey179/vfsstream": "^1.6.11"
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/",
            "App\\Tests\\": "app/tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "project-dependencies-installed": {
            "composer-plugin-api": "^2",
            "php": "^8.1"
        }
    },
    "provide": {
        "silverstripe/recipe-plugin": "2.0.0"
    }
}
